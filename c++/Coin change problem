#include <bits/stdc++.h>
using namespace std;

int solve(vector<int> &num, int x) {
    if (x == 0) {
        return 0;
    }
    if (x < 0) {
        return INT_MAX; // Impossible to form the amount
    }

    int mini = INT_MAX;
    for (int i = 0; i < num.size(); i++) {
        int ans = solve(num, x - num[i]);
        if (ans != INT_MAX) {
            // If the result is valid, consider the current coin
            mini = min(mini, 1 + ans);
        }
    }
    return mini;
}

int minimumElements(vector<int> &num, int x) {
    int ans = solve(num, x);
    return (ans == INT_MAX) ? -1 : ans;
}

int main() {
    vector<int> num = {1, 2, 3}; // Example input vector
    int x = 5; // Example value of x
    int result = minimumElements(num, x);
    cout << "Minimum number of elements required to form " << x << " is: " << result << endl;
    return 0;
}
